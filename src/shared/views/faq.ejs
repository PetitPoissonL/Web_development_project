<body>
  <!-- Navigation-->
  <%- include('./navbar'); %>
    <div class="text-center">
      <h2 class="mt-5 mb-5 text-secondary">FAQ</h2>
    </div>
    <section class="container my-5" id="maincontent">
      <a class="py-3 d-block h-100 w-100 position-relative z-index-1 pr-1 text-secondary border-top"
        aria-controls="faq-17" aria-expanded="false" data-toggle="collapse" href="#faq-17" style="text-decoration:none">
        <i class="bi bi-bookmark-plus-fill fs-3"></i>
        <span class="fs-2"> &nbsp Who are we ?</span>
